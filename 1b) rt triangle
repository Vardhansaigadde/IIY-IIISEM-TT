#include <stdio.h>

int main() {
    int rows, i, j;

    // Input number of rows
    printf("Enter number of rows: ");
    scanf("%d", &rows);

    // Outer loop for rows
    for (i = 1; i <= rows; i++) {
        // Inner loop for stars
        for (j = 1; j <= i; j++) {
            printf("* ");
        }
        printf("\n"); // Move to the next line
    }

    return 0;
}
